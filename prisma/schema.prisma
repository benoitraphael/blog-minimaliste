datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int      @id @default(autoincrement())
  username String   @unique
  password String
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  date      DateTime @default(now())
  tags      String?
  published Boolean  @default(true)
}
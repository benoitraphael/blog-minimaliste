datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int      @id @default(autoincrement())
  username String   @unique
  password String
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  date      DateTime @default(now())
  tags      String?
  published Boolean  @default(true)
}